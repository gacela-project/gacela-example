{
    "description": "A basic PHP scaffolding",
    "name": "chemaclass/php-scaffolding",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "php",
        "scaffolding"
    ],
    "require": {
        "php": ">=7.4",
        "gacela-project/gacela": "@stable"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
